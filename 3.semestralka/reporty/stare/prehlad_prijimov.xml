	<query>
		<sql><![CDATA[SELECT CENA, DATUM
      FROM PLATBA
INNER JOIN HISTORIA_PARKOVANIA HP
     USING (ID_HISTORIA)
INNER JOIN PARKOVACIE_MIESTO PM
     USING (ID_MIESTA)
INNER JOIN ZONA
     USING (ID_ZONA)
     WHERE DATUM BETWEEN :DATUM_OD AND :DATUM_DO
       AND ZONA.TYP_VOZIDLO = :TYP_VOZIDLO
       AND ZONA.TYP_ZONA = :TYP_ZONA]]></sql>
		<binds>
